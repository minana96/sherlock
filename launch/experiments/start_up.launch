<launch>
  <!-- Arguments -->
  <arg name="frequency" default="10.0"/>

  <!-- turtlebot3_robot -->
  <include file="$(find sherlock_bringup)/launch/sherlock_bringup.launch"/>

  <!-- raspicam_node -->
  <include file="$(find raspicam_node)/launch/camerav2_640x480_20.launch"/>

  <!-- resource_profiler -->
  <node pkg="ros_melodic_profilers" type="cpu_mem_profiler_server.py" name="resource_profiler" output="screen">
    <param name="frequency" value="$(arg frequency)" />
  </node>

  <!-- ina219_profiler-->
  <node pkg="ros_melodic_profilers" type="ina219_profiler_server.py" name="ina219_profiler" output="screen"/>

  <!-- sherlock_obj_recognition_results -->
  <node pkg="sherlock" type="sherlock_obj_recognition_results.py" name="sherlock_obj_recognition" output="screen"/>
</launch>